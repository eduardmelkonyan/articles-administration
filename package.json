    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "migrate": "^1.8.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "objection": "^3.0.1",
    "pg": "^8.8.0",
    "sharp": "^0.31.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
