{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "make-seed": "npx knex seed:make dev --knexfile ./db/knexfile.js",
    "dev": "env-cmd -f ./config/dev.env nodemon app.js",
